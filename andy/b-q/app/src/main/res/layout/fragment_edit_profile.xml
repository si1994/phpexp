                    android:gravity="start"
                    android:hint="@string/hint_identity_text"
                    android:maxLength="150"
                    android:padding="@dimen/_10sdp"
                    app:emojiSize="@dimen/_18sdp"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/_12sdp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:background="@color/colorHintOnBoard" />

                <TextView
                    style="@style/font_sf_pro_text_semibold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_10sdp"
                    android:gravity="start"
                    android:text="@string/my_interests"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/_12sdp" />

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rcvMyInterests"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_marginBottom="@dimen/_10sdp">

                </android.support.v7.widget.RecyclerView>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:background="@color/colorHintOnBoard" />

                <TextView
                    style="@style/font_sf_pro_text_semibold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_10sdp"
                    android:gravity="start"
                    android:text="@string/my_social_life"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/_12sdp" />

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rcvSocialLifeList"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp">

                </android.support.v7.widget.RecyclerView>

