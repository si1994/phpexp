                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:background="@color/colorHintOnBoard" />

                <TextView
                    style="@style/font_sf_pro_text_semibold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_10sdp"
                    android:gravity="start"
                    android:text="@string/my_ethnicity"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/_12sdp" />

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/rcvMyEthnicity"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_marginBottom="@dimen/_10sdp" />


                <TextView
                    style="@style/font_sf_pro_text_bold"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary"
                    android:gravity="start"
                    android:paddingStart="@dimen/_10sdp"
                    android:paddingTop="@dimen/_6sdp"
                    android:paddingBottom="@dimen/_6sdp"
                    android:text="@string/connected_accounts"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/_14sdp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:background="@color/colorHintOnBoard" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp">

                    <TextView
                        style="@style/font_sf_pro_text_regular"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="start"
                        android:padding="@dimen/_5sdp"
                        android:text="@string/facebook"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp" />

                    <ToggleButton
                        android:id="@+id/tbvFacebook"
                        android:layout_width="@dimen/_60sdp"
                        android:layout_height="@dimen/_22sdp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/_12sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_12sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:background="@drawable/ic_switch_custom"
                        android:checked="true"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:textOff=""
                        android:textOn="" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5sdp">

                    <TextView
                        style="@style/font_sf_pro_text_regular"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="left"
                        android:padding="@dimen/_5sdp"
                        android:text="@string/instagram"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_12sdp" />

                    <ToggleButton
                        android:id="@+id/tbvInstagram"
                        android:layout_width="@dimen/_60sdp"
                        android:layout_height="@dimen/_22sdp"
